{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@": ["src"],
      }
    },
    "exclude": ["node_modules"]
  }